<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
                 xmlns:aapt="http://schemas.android.com/aapt">

    <aapt:attr name="android:drawable">
        <vector android:width="24dp"
                android:height="24dp"
                android:viewportWidth="24"
                android:viewportHeight="24"
                android:alpha="0.54">
            <group
                    android:name="group"
                    android:pivotX="12"
                    android:pivotY="12"
                    android:rotation="180">
                <path
                        android:name="clear_top_right"
                        android:pathData="M 12 12 L 18.3 5.7 Z"
                        android:strokeColor="@color/grey_900"
                        android:strokeWidth="2"
                        android:strokeLineCap="butt"
                        android:strokeLineJoin="miter"
                        android:strokeMiterLimit="10"/>
                <path
                        android:name="clear_bottom_right"
                        android:pathData="M 12 12 L 18.3 18.3 Z"
                        android:strokeColor="@color/grey_900"
                        android:strokeWidth="2"
                        android:strokeLineCap="butt"
                        android:strokeLineJoin="miter"
                        android:strokeMiterLimit="10"/>
                <path
                        android:name="arrow_head_top"
                        android:pathData="M 12.71 12.71 L 4.7 4.7 Z"
                        android:strokeColor="@color/grey_900"
                        android:strokeWidth="2"
                        android:trimPathEnd="0.44"
                        android:strokeLineCap="butt"
                        android:strokeLineJoin="miter"
                        android:strokeMiterLimit="10"/>
                <path
                        android:name="arrow_head_bottom"
                        android:pathData="M 12.71 11.29 L 4.7 19.3 Z"
                        android:strokeColor="@color/grey_900"
                        android:strokeWidth="2"
                        android:trimPathEnd="0.44"
                        android:strokeLineCap="butt"
                        android:strokeLineJoin="miter"
                        android:strokeMiterLimit="10"/>
                <path
                        android:name="stem"
                        android:pathData="M 11.41 12 L -2.59 12 Z"
                        android:strokeColor="@color/grey_900"
                        android:strokeWidth="2"
                        android:trimPathStart="1"
                        android:strokeLineCap="butt"
                        android:strokeLineJoin="miter"
                        android:strokeMiterLimit="10"/>
            </group>
        </vector>
    </aapt:attr>

    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="group"
                        android:propertyName="translateX"
                        android:duration="300"
                        android:valueFrom="0"
                        android:valueTo="-7.3"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/decelerate_interpolator"/>

                <!--with 180 rotation-->
                <!--<objectAnimator
                        android:name="group"
                        android:propertyName="rotation"
                        android:duration="300"
                        android:valueFrom="0"
                        android:valueTo="180"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>-->
            </set>
        </aapt:attr>
    </target>

    <target android:name="stem">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="stem"
                        android:propertyName="trimPathStart"
                        android:startOffset="100"
                        android:duration="200"
                        android:valueFrom="1"
                        android:valueTo="0"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="clear_top_right">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="clear_bottom_right"
                        android:propertyName="trimPathStart"
                        android:duration="150"
                        android:valueFrom="0"
                        android:valueTo="1"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="clear_bottom_right">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="clear_bottom_right"
                        android:propertyName="trimPathStart"
                        android:duration="150"
                        android:valueFrom="0"
                        android:valueTo="1"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="arrow_head_top">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="head_bottom"
                        android:propertyName="trimPathEnd"
                        android:startOffset="150"
                        android:duration="150"
                        android:valueFrom="0.44"
                        android:valueTo="1"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>

    <target android:name="arrow_head_bottom">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                        android:name="head_bottom"
                        android:propertyName="trimPathEnd"
                        android:startOffset="150"
                        android:duration="150"
                        android:valueFrom="0.44"
                        android:valueTo="1"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>


